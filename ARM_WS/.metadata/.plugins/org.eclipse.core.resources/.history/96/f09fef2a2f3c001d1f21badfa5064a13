#ifndef GPIO_CNFG_H
#define GPIO_CNFG_H

#include "GPIO_interface.h"

#define NUMBER_OF_USED_PINS  8

typedef struct
{
	PORT_NAME PORT;
	PIN_NUM PIN;
	PIN_MODE MODE;
	AF_ID AF
	const PUSHPULL_OR_OPENDRAIN TYPE;
	const OUTPUT_SPEED SPEED;
	PULL_UP_DOWN SETTING;
}pinInfo;




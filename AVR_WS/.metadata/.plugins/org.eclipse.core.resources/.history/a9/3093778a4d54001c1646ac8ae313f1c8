#include "LIB/bitmath.h"
#include"LIB/stdtypes.h"
#include "avr/io.h"
#include "avr/delay.h"
void main (void)
{
	DDRA=0xff;
	for(int i =0 ;i<512 ;i++)
	{
		for(int j = 1 ; j <=4 ;j++)
		{
			PORTA=0x00;
			PORTA |= (1>>j) ;
			_delay_ms(20);
		}
	}
}

#include "LIB/bitmath.h"
#include"LIB/stdtypes.h"
#include "avr/io.h"
#include "avr/delay.h"
void main (void)
{
	DDRA=0xff;
	WDTCR = 0b00000111 ;
//	WDTCR |= 0b00001000;
	PORTA=0xff;
	_delay_ms(500);
	PORTA=0x00;
	while(1)
	{

	}
}

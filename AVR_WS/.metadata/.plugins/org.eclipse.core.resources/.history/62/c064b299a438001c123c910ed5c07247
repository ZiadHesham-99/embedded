#include"LIB/stdtypes.h"
#include"LIB/bitmath.h"
#include"AVR/io.h"
#include"avr/interrupt.h"
#define NORMAL_MODE_SET_MASK (u8) 0b10110000
#define PRESCALLER_1024_SET_MASK (u8) 0b10110101

ISR()
{

}
void main void()
{
	DDRC = 0b00010000;
	TCCR0 & =  NORMAL_MODE_SET_MASK ;
	SET_BIT(TIMSK,0);



}
